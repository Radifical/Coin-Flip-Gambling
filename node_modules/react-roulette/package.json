  "bugs": {
    "url": "https://gitlab.com/matias.bontempo/react-roulette/issues"
  },
  "homepage": "https://gitlab.com/matias.bontempo/react-roulette#readme",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-watch": "^3.1.3",
    "markdown-styles": "^3.1.10",
    "postcss-nested": "^4.1.2",
    "styled-jsx": "^3.2.1",
